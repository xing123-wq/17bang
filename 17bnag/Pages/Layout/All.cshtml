                                <li><a href="/Problems/New"><span class="fa fa-edit"></span> 发布</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span zyf-ducu-notices="" class="fa fa-eye"></span> 督促</a>
                            <ul class="dropdown-menu" role="menu" zyf-navigator-ducu="">
                                <li><a href="/Plan/New"><span class="fa fa-edit fa-fw"></span> 求督促</a></li>
                                <li><a href="/Plan"><span class="fa fa-gavel fa-fw"></span> 督促别人</a></li>
                                <li><a href="/Plan?my=f&amp;full=False&amp;due=False&amp;expire=False&amp;Frozen=False"><span class="fa fa-list fa-fw"></span> 我的目标</a></li>
                                <li><a href="/Plan?my=s&amp;full=False&amp;due=False&amp;expire=True&amp;Frozen=False"><span class="fa fa-list fa-fw"></span> 我的督促</a></li>
                            </ul>
                        </li>
                        @if (ViewData["UserName"] == null)
                        {
                            <li class="dropdown">
                                <a href="/Article" class="dropdown-toggle" data-toggle="dropdown"><span class="iconfont icon-articles"></span> 文章</a>
                                <ul class="dropdown-menu" role="menu">
                                    <li><a href="/Article"><span class="fa fa-list fa-fw"></span> 列表</a> </li>
                                </ul>
