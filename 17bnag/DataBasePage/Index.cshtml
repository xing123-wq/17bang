@page
@model _17bnag.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].Body)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].KeyWord)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].Resort)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].Moneys)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HelpRelease[0].PublishDateTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.HelpRelease) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Body)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KeyWord)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Resort)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Moneys)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublishDateTime)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
